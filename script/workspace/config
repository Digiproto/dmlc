QEMU_DIR=$HOME/qemu/qemu
QEMU_INSTALL_DIR=$HOME/qemu/qemu
SYSTEMC_DIR=/opt/dml_all
SYSTEMC_INSTALL_DIR=/opt/dml_all
QEMU_CONF_FLAGS="--target-list=arm-softmmu --enable-dml"
SYSTEMC_CONF_FLAGS=""
DMLC=dmlc
PYTHON=python

QEMU_BUILD=yes
SYSTEMC_BUILD=yes
